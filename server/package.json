{
  "name": "server",
  "version": "0.0.11",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "set DATABASE_HOST=localhost&& npm run prisma:seed && jest --config ./test/jest-e2e.json",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:create": "set DATABASE_HOST=localhost&& npx prisma migrate dev --create-only",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:deploy:test": "set DATABASE_HOST=localhost&& npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "set DATABASE_HOST=localhost&& npx prisma db seed",
    "email:dev": "email dev -p 3001",
    "test:e2e:dev": "bash ./scripts/run-integration.sh && npm run migrate:deploy:test && npm run prisma:seed && npm run test:e2e",
    "prisma:seed:prod": "node dist/prisma/seed.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.554.0",
